{
  "plugins": [
    "import",
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended"
  ],
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.app.json"
      }
    }
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.app.json"
  },
  "rules": {
    "import/no-unresolved": "error",
    "import/named": "error"
  }
}